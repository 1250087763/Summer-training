#include<stdio.h>
int main(void)
{
    int n;
    int a[12]= {31,28,31,30,31,30,31,31,30,31,30,31};
    scanf("%d",&n);
    while(n--)
    {
        int year,month,day,s;
        scanf("%d%d%d",&year,&month,&day);
        if(month==1)
        {
            printf("%d\n",day);
        }
        else if(month==2)
        {
            printf("%d\n",day+31);
        }
        else
        {
            s=0;
            for(int i=0;i<month-1;i++)
            {
                s=s+a[i];
            }
            s=s+day;
            if((year%4==0&&year%100!=0)||(year%400==0))
            {
                s=s+1;
            }
            printf("%d\n",s);
        }
    }
    return 0;
}
