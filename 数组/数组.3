//输入10个数到数组t中，再输入x，如果有与x相等的数组元素，输出该数组元素的下标；否则，输出-1
#include<stdio.h>
int main(void)
{
    int i, eq ;
    double  x, t[10];
    printf("Input 10 numbers: ");
    for ( i = 0; i < 10; i++)
        scanf("%lf", &t[i]);
    printf("Input x: ");
    scanf("%lf", &x);
    for (i = 0; i < 10; i++)
         if(t[i] == x) {
            eq = i;
            printf("%d\n", eq);
         }
    if (i > 9)
         printf("-1\n");

    return 0;
   }
